{
     "manifestVersion": 1,
    "id": "chatdown-task",
    "name": "Chatdown",
    "version": "0.0.1",
    "publisher": "vgrycuk-testing",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Chatdown is a transcript generator which consumes a .chat file to generate mock transcripts.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "source"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "source"
            }
        }
    ]
}