{
     "manifestVersion": 1,
    "id": "msbot-clone",
    "name": "msbot-clone",
    "version": "0.0.1",
    "publisher": "vgrycuk-testing",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Clones a bot to Azure Portal using MSBot Clone.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "source"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "source"
            }
        }
    ]
}