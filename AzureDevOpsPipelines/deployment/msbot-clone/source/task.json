{
    "id": "2fc164ff-2fa7-477d-b89d-78be98fdb584",
    "name": "msbot-clone",
    "friendlyName": "msbot-clone",
    "description": "This commands then creates {dispatchName}.dispatch file.",
    "helpMarkDown": "",
    "category": "Utility",
    "author": "Southworks-pipelines-test",
    "version": {
        "Major": 0,
        "Minor": 1,
        "Patch": 0
    },
    "instanceNameFormat": "Bot Service $(commandlist)",
    "inputs": [
        {
            "name": "connectedServiceNameARM",
            "aliases": [
                "azureSubscription"
            ],
            "type": "connectedService:AzureRM",
            "label": "Azure subscription",
            "required": true,
            "helpMarkDown": "Select an Azure resource manager subscription for the deployment"
        },
        {
            "name": "proj-file",
            "type": "text",
            "label": "Project File Name",
            "required": true,
            "helpMarkDown": ""
        },
        {
            "name": "code-dir",
            "type": "filepath",
            "label": "Root Folder",
            "required": true,
            "helpMarkDown": ""
        },
        {
            "name": "name",
            "type": "text",
            "label": "Name of the Bot",
            "required": true,
            "helpMarkDown": ""
        },
        {
            "name": "location",
            "type": "picklist",
            "label": "Location",
            "required": true,
            "helpMarkDown": "",
            "options": {
                "westus": "Westus"
            }
        },
        {
            "name": "sdkLanguage",
            "type": "picklist",
            "label": "SDK Language",
            "required": true,
            "helpMarkDown": "",
            "options": {
                "Csharp": "C#",
                "Node": "NodeJS"
            }
        },
        {
            "name": "folder",
            "type": "filepath",
            "label": "Folder",
            "required": true,
            "helpMarkDown": ""
        },
        {
            "name": "appId",
            "type": "text",
            "label": "App ID",
            "required": true,
            "helpMarkDown": ""
        },
        {
            "name": "appSecret",
            "type": "text",
            "label": "App Secret",
            "required": true,
            "helpMarkDown": ""
        },
        {
            "name": "luisAuthoringKey",
            "type": "text",
            "label": "LUIS Key",
            "required": false,
            "helpMarkDown": ""
        }
    ],
    "execution": {
        "Node": {
            "target": "index.js"
        }
    }
}